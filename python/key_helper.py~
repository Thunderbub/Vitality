#!/usr/bin/python
print "Hello World!"

pygame.init()
  
    # set up key repeating so we can hold down the key to scroll.
    old_k_delay, old_k_interval = pygame.key.get_repeat ()
    pygame.key.set_repeat (500, 30)

finally:
        pygame.key.set_repeat (old_k_delay, old_k_interval)
        pygame.quit()


